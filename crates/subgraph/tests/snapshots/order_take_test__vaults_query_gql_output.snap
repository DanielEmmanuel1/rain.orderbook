query OrderTakeDetailQuery($id: ID!) {
  trade(id: $id) {
    tradeEvent {
      transaction {
        id
        from
      }
    }
    outputVaultBalanceChange {
      amount
      vault {
        token {
          id
          address
          name
          symbol
          decimals
        }
      }
    }
    order {
      orderHash
    }
    inputVaultBalanceChange {
      vault {
        token {
          id
          address
          name
          symbol
          decimals
        }
      }
      amount
    }
  }
}
