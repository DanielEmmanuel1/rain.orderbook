---
source: crates/subgraph/tests/order_test.rs
expression: request_body.query
---
query OrderDetailQuery($id: ID!) {
  order(id: $id) {
    orderBytes
    orderHash
    owner
    outputs {
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    inputs {
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    active
    addEvents(first: 1, orderBy: transaction__timestamp, orderDirection: desc) {
      transaction {
        blockNumber
        timestamp
      }
    }
    meta
  }
}
