---
source: crates/subgraph/tests/multi_order_test.rs
expression: request_body.query
---
query MultiOrderDetailQuery($filter: Order_filter!) {
  orders(where: $filter) {
    id
    orderBytes
    orderHash
    owner
    outputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    inputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    active
    addEvents {
      transaction {
        blockNumber
        timestamp
      }
    }
    meta
    timestampAdded
  }
}
