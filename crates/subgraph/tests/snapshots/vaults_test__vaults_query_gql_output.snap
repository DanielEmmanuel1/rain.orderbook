---
query VaultsListQuery($first: Int, $skip: Int) {
  vaults(orderBy: id, orderDirection: desc, skip: $skip, first: $first) {
    id
    vaultId
    owner
    token {
      id
      address
      name
      symbol
      decimals
    }
    balance
    ordersAsInput {
      id
      orderHash
    }
    ordersAsOutput {
      id
      orderHash
    }
  }
}
