---
source: crates/subgraph/tests/vault_balance_changes_test.rs
expression: request_body.query
---
query VaultBalanceChangesListQuery($first: Int, $id: Bytes!, $skip: Int) {
  vaultBalanceChanges(orderDirection: desc, where: {vault_: {id: $id, }, }, skip: $skip, first: $first) {
    __typename
    amount
    newVaultBalance
    oldVaultBalance
    vault {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
    }
    timestamp
    transaction {
      id
      from
    }
  }
}
